<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxlogin.JavaFXLoginController">
   <children>
      <Label fx:id="errormsg" layoutX="272.0" layoutY="253.0" prefHeight="43.0" prefWidth="263.0" textFill="#eb1f09">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="btnusername" layoutX="272.0" layoutY="106.0" prefHeight="43.0" prefWidth="263.0" promptText="Username">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <PasswordField fx:id="btnpassword" layoutX="272.0" layoutY="175.0" prefHeight="43.0" prefWidth="263.0" promptText="Password">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <Button fx:id="btnlogin" layoutX="379.0" layoutY="309.0" mnemonicParsing="false" onAction="#CheckLogin" prefHeight="54.0" prefWidth="156.0" text="Log In">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
